{
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "mapRoot": "",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    /*"noUnusedParameters": true,
    "noUnusedLocals": true,*/
    "outDir": "./dist",
    "rootDir": "",
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es6",
    "lib": [
      "es2017"
    ],
    "baseUrl": "",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "jasmine"
    ],
    "paths": {
      "*": [
        "src/@types/*"
      ]
    }
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}